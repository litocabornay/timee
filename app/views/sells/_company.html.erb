<!--セル情報の一覧のフォーム(並び替え可能)-->
<br>
<h3>受付連絡先情報</h3>
<div class="width_table">
<table class="table font-small sampleTable">

<tr scope="row"> 
  <th width="50"  class="thead-inverse">メインの受付用電話番号</th>
  <td width="150" ><%= @sell.user.company_call_number_contact %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">受付時間</th>
  <td width="150" ><%= @sell.user.company_call_time_from_1 %>時
        <%= @sell.user.company_call_time_from_2 %>分　〜　
        <%= @sell.user.company_call_time_to_1 %>時
        <%= @sell.user.company_call_time_to_2 %>分</td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">メインの受付電話番号者</th>
  <td width="150" ><%= @sell.user.company_reception %>（<%= @sell.user.company_reception_sound %>）</td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">電話番号者の役職</th>
  <td width="150" ><%= @sell.user.company_position %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">受付用FAX番号</th>
  <td width="150" ><%= @sell.user.company_fax_number_contact %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">受付用メールアドレス</th>
  <td width="150" ><%= @sell.user.company_mail_address %></td>
<tr>

<tr scope="row"> 
  <th width="50"  class="thead-inverse">受付先住所</th>
  <td width="150" ><%= @sell.user.company_post_number_contact %>
      <%= @sell.user.company_place_contact %>
      <%= @sell.user.company_place_detail_contact %></td>
<tr>
  
</table>
</div>
<div class="width_table">
<table class="table font-small sampleTable">
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">その他連絡先①</th>
  <td width="150" >担当：<%= @sell.user.company_call_number_contact_name_1 %><br>
      電話番号：<%= @sell.user.company_call_number_contact_tel_1 %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">その他連絡先②</th>
  <td width="150" >担当：<%= @sell.user.company_call_number_contact_name_2 %><br>
      電話番号：<%= @sell.user.company_call_number_contact_tel_2 %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">その他連絡先③</th>
  <td width="150" >担当：<%= @sell.user.company_call_number_contact_name_3 %><br>
      電話番号：<%= @sell.user.company_call_number_contact_tel_3 %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">その他連絡先④</th>
  <td width="150" >担当：<%= @sell.user.company_call_number_contact_name_4 %><br>
      電話番号：<%= @sell.user.company_call_number_contact_tel_4 %></td>
<tr>
  
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">その他連絡先⑤</th>
  <td width="150" >担当：<%= @sell.user.company_call_number_contact_name_5 %><br>
      電話番号：<%= @sell.user.company_call_number_contact_tel_5 %></td>
<tr>
  
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">その他連絡先⑥</th>
  <td width="150" >担当：<%= @sell.user.company_call_number_contact_name_6 %><br>
      電話番号：<%= @sell.user.company_call_number_contact_tel_6 %></td>
<tr>
  
</table>
</div>
<br>
<h3>販売会社情報</h3>
<div class="width_table">
<table class="table font-small sampleTable">

<tr scope="row"> 
  <th width="50"  class="thead-inverse">販売社名</th>
  <td width="150" ><%= @sell.user.company_name %> (<%= @sell.user.company_name_sounds %>)</td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">業態</th>
  <td width="150" ><%= @sell.user.company_type %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">所属組合</th>
  <td width="150" ><%= @sell.user.company_union %></td>
<tr>

<tr scope="row"> 
  <th width="50"  class="thead-inverse">支店</th>
  <td width="150" ><%= @sell.user.company_branch %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">代表者名</th>
  <td width="150" ><%= @sell.user.company_president %> (<%= @sell.user.company_president_sound %>)</td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">住所</th>
  <td width="150" >
  （<%= @sell.user.company_post_number %>）
  <%= @sell.user.company_place %>　
  <%= @sell.user.company_place_detail %>
  </td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">電話番号</th>
  <td width="150" ><%= @sell.user.company_call_number %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">FAX番号</th>
  <td width="150" ><%= @sell.user.company_fax_number %></td>
<tr>

<tr scope="row"> 
  <th width="50"  class="thead-inverse">緊急連絡先</th>
  <td width="150" ><%= @sell.user.company_call_number_emergency %></td>
<tr>
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">登録日</th>
  <td width="150" ><%= @sell.user.created_at.strftime("%y/%m/%d") %></td>
<tr>
  
</table> 
</div>
<br>
<h3>そのほか会社情報</h3>
<div class="width_table">
<table class="table font-small sampleTable">
  
<tr scope="row"> 
  <th width="50"  class="thead-inverse">公式WEBページ</th>
  <td width="150" ><%= @sell.user.company_url %></td>
<tr>

<tr scope="row"> 
  <th width="50"  class="thead-inverse">会社PR</th>
  <td width="150" ><%= @sell.user.company_pr %></td>
